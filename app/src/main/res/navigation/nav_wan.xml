        tools:layout="@layout/project_fragment" />
    <fragment
        android:id="@+id/navigation_tree"
        android:name="me.pakhang.wanandroid.ui.tree.TreeFragment"
        android:label="tree_fragment"
        tools:layout="@layout/tree_fragment" />
    <fragment
        android:id="@+id/navigation_navi"
        android:name="me.pakhang.wanandroid.ui.navi.NaviFragment"
        android:label="navi_fragment"
        tools:layout="@layout/navi_fragment" />

    <fragment
        android:id="@+id/navigation_collect"
        android:name="me.pakhang.wanandroid.ui.collect.CollectFragment"
